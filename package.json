{
  "name": "mono-mongodb",
  "version": "2.4.1",
  "description": "MongoDB module for Mono",
  "main": "lib/",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint lib/**",
    "test": "npm run lint && nyc ava --verbose --serial test/ && nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
