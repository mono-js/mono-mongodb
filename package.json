    "url": "git+https://github.com/terrajs/mono-mongodb.git"
  },
  "nyc": {
    "include": [
      "lib/"
    ]
  },
  "contributors": [
    {
      "name": "Sebastien Chopin (@Atinux)"
    },
    {
      "name": "Benjamin Canac (@benjamincanac)"
    },
    {
      "name": "Gaetan SENN (@gaetansenn)"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "joi": "^13.0.1",
    "mongodb": "^3.0.4"
  },
  "devDependencies": {
    "mono-core": "1.0.0",
    "ava": "0.25.0",
    "codecov": "3.0.4",
    "eslint": "5.4.0",
    "mongodb-utils": "1.1.6",
    "mono-test-utils": "1.2.4",
    "nyc": "12.0.2",
    "rimraf": "2.6.2",
    "std-mocks": "1.0.1"
